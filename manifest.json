{
  "manifest_version": 3,
  "name": "Home Free Accessibility Extension",
  "version": "1.0",

"description": "Displays accessibility and mobility info for properties on realestate.com.au.",
  "host_permissions": [
    "*://*.realestate.com.au/*",
    "https://home-free-api.onrender.com/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "*://*.realestate.com.au/*"
      ],
      "js": ["content.js"]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": ["logo.png"],
      "matches": ["*://*.realestate.com.au/*"]

    }
  ]
}


